<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/loginFragment">

    <fragment
        android:id="@+id/loginFragment"
        android:name="com.kitabeli.ae.ui.login.LoginFragment"
        android:label="LoginFragment"
        tools:layout="@layout/fragment_login">

        <action
            android:id="@+id/action_loginFragment_to_homeFragment"
            app:destination="@id/fragment_post_login"
            app:launchSingleTop="true"
            app:popUpTo="@id/loginFragment"
            app:popUpToInclusive="true" />

        <action
            android:id="@+id/action_loginFragment_to_kioskFragment"
            app:destination="@id/kiosFragment"
            app:launchSingleTop="true"
            app:popUpTo="@id/loginFragment"
            app:popUpToInclusive="true" />

        <action
            android:id="@+id/action_loginFragment_to_addCheckStockFragment"
            app:destination="@id/addCheckStockFragment"
            app:launchSingleTop="true"
            app:popUpTo="@id/loginFragment"
            app:popUpToInclusive="true" />

        <action
            android:id="@+id/action_to_stockOpOnBoardingFragment"
            app:destination="@id/fragment_stock_op_on_boarding"
            app:launchSingleTop="true"
            app:popUpTo="@id/loginFragment"
            app:popUpToInclusive="true" />

    </fragment>

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.kitabeli.ae.ui.home.HomeFragment"
        android:label="HomeFragment"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeFragment_to_kiosFragment"
            app:destination="@id/kiosFragment" />

        <action
            android:id="@+id/action_homeFragment_to_addCheckStockFragment"
            app:destination="@id/addCheckStockFragment" />


        <action
            android:id="@+id/action_homeFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:launchSingleTop="true"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />

        <action
            android:id="@+id/action_homeFragment_to_scanqrFragment"
            app:destination="@id/scanQRFragment" />

        <action
            android:id="@+id/action_to_stock_withdrawal"
            app:destination="@id/fragment_stock_withdrawal"
            app:launchSingleTop="false"
            app:popUpTo="@id/homeFragment" />


    </fragment>

    <fragment
        android:id="@+id/kiosFragment"
        android:name="com.kitabeli.ae.ui.kios.KiosFragment"
        android:label="KiosFragment"
        tools:layout="@layout/fragment_kios">

        <action
            android:id="@+id/action_kiosFragment_to_addCheckStockFragment"
            app:destination="@id/addCheckStockFragment"
            app:launchSingleTop="true"
            app:popUpTo="@id/kiosFragment"
            app:popUpToInclusive="true" />

        <action
            android:id="@+id/action_kiosFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:launchSingleTop="true"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />

        <argument
            android:name="stockOpNameId"
            app:argType="integer" />

    </fragment>

    <fragment
        android:id="@+id/addCheckStockFragment"
        android:name="com.kitabeli.ae.ui.addcheckStock.AddCheckStockFragment"
        android:label="AddCheckStockFragment"
        tools:layout="@layout/fragment_add_check_stock">

        <action
            android:id="@+id/action_addCheckStockFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:launchSingleTop="true"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />

        <action
            android:id="@+id/action_addCheckStockFragment_to_kiosFragment"
            app:destination="@id/kiosFragment"
            app:launchSingleTop="true"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />

        <action
            android:id="@+id/action_addCheckStockFragment_to_loginFragment"
            app:destination="@id/loginFragment"
            app:launchSingleTop="true"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />

        <action
            android:id="@+id/action_to_stockOpOnBoardingFragment"
            app:destination="@id/fragment_stock_op_on_boarding"
            app:launchSingleTop="true"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />

        <action
            android:id="@+id/action_to_paymentDetailFragment"
            app:destination="@id/fragment_payment_detail"
            app:launchSingleTop="false"
            app:popUpTo="@id/addCheckStockFragment" />

        <action
            android:id="@+id/action_to_stock_withdrawal"
            app:destination="@id/fragment_stock_withdrawal"
            app:launchSingleTop="false"
            app:popUpTo="@id/homeFragment">

        </action>

        <argument
            android:name="stockOpNameId"
            app:argType="integer" />

        <argument
            android:name="isFromPartialPayment"
            android:defaultValue="false"
            app:argType="boolean" />


    </fragment>

    <fragment
        android:id="@+id/signatureFragment"
        android:name="com.kitabeli.ae.ui.signature.SignatureFragment"
        android:label="SignatureFragment"
        tools:layout="@layout/fragment_signature" />

    <fragment
        android:id="@+id/photoViewFragment"
        android:name="com.kitabeli.ae.ui.photoview.PhotoViewFragment"
        android:label="PhotoViewFragment"
        tools:layout="@layout/fragment_photo_view" />

    <fragment
        android:id="@+id/scanQRFragment"
        android:name="com.kitabeli.ae.ui.scanqr.ScanQrFragment"
        android:label="ScanQrFragment"
        tools:layout="@layout/fragment_scanqr" />

    <fragment
        android:id="@+id/fragment_post_login"
        android:name="com.kitabeli.ae.ui.post_login.PostLoginFragment"
        android:label="PostLogin"
        tools:layout="@layout/fragment_post_login">

        <action
            android:id="@+id/action_to_home_fragment"
            app:destination="@id/homeFragment"
            app:launchSingleTop="true"
            app:popUpToInclusive="true" />

        <action
            android:id="@+id/action_to_training_video"
            app:destination="@id/fragment_training_video"
            app:launchSingleTop="true"
            app:popUpToInclusive="true" />

    </fragment>

    <fragment
        android:id="@+id/fragment_training_video"
        android:name="com.kitabeli.ae.ui.training_video.TrainingVideoFragment"
        android:label="Training Video"
        tools:layout="@layout/fragment_training_video">

        <action
            android:id="@+id/action_to_video_player"
            app:destination="@id/fragment_training_video"
            app:launchSingleTop="true"
            app:popUpToInclusive="true">

            <argument
                android:name="videoId"
                app:argType="string" />

        </action>

    </fragment>

    <fragment
        android:id="@+id/fragment_video_player"
        android:name="com.kitabeli.ae.ui.training_video.VideoFragment"
        android:label="Video"
        tools:layout="@layout/fragment_video_player">


    </fragment>


    <fragment
        android:id="@+id/fragment_maintenance"
        android:name="com.kitabeli.ae.ui.MaintenanceFragment"
        android:label="MaintenanceFragment" />

    <fragment
        android:id="@+id/fragment_stock_op_on_boarding"
        android:name="com.kitabeli.ae.ui.onboarding.StockOpOnBoardingFragment"
        android:label="StockOpOnBoardingFragment">

        <action
            android:id="@+id/action_to_kioskFragment"
            app:destination="@id/kiosFragment"
            app:launchSingleTop="true"
            app:popUpTo="@id/nav_graph"
            app:popUpToInclusive="true" />

    </fragment>

    <fragment
        android:id="@+id/fragment_payment_detail"
        android:name="com.kitabeli.ae.ui.addcheckStock.PaymentDetailFragment"
        android:label="PaymentDetailFragment" />

    <fragment
        android:id="@+id/fragment_shutdown_and_return"
        android:name="com.kitabeli.ae.ui.resign_and_return.ResignAndReturnFragment"
        android:label="ResignAndReturnFragment" />

    <fragment
        android:id="@+id/fragment_resign_form"
        android:name="com.kitabeli.ae.ui.resign_and_return.kiosk_resign.KioskResignFormFragment"
        android:label="ResignFormFragment" />

    <fragment
        android:id="@+id/fragment_return_request"
        android:name="com.kitabeli.ae.ui.resign_and_return.return_request.ReturnRequestFragment"
        android:label="ReturnRequestFragment" />

    <fragment
        android:id="@+id/fragment_stock_withdrawal"
        android:name="com.kitabeli.ae.ui.stock_withdrawal.FragmentStockWithdrawal"
        android:label="FragmentStockWithdrawal"
        app:popUpTo="@id/homeFragment"
        app:popUpToInclusive="true">

        <argument
            android:name="stockTransferId"
            app:argType="string" />

    </fragment>

</navigation>